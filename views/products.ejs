<%- include('header'); -%>

    <div class="card-group">
        <div class="card">
            <img
                class="card-img-top"
                src="/mortar.png"
                alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Card title</h5>
                    <p class="card-text">This is a wider card with supporting text below as a
                        natural lead-in to additional content. This content is a little bit longer.</p>
                    <button class="sub-btn" onclick="cart()">Add to Cart</button>
                    <p class="card-text">
                        <small class="text-muted">Last updated 3 mins ago</small>
                    </p>
                </div>
            </div>
            <div class="card">
                <img
                    class="card-img-top"
                    src="/cement.png"
                    alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">Cement</h5>
                        <p class="card-text">Acc cement</p>
                        <button class="sub-btn">Add to Cart</button>
                        <p class="card-text">
                            <small class="text-muted">Last updated 3 mins ago</small>
                        </p>
                    </div>
                </div>
                <div class="card">
                    <img
                        class="card-img-top"
                        src="/steel.png"
                        alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">Card title</h5>
                            <p class="card-text">This is a wider card with supporting text below as a
                                natural lead-in to additional content. This card has even longer content than
                                the first to show that equal height action.</p>
                            <button class="sub-btn">Add to Cart</button>
                            <p class="card-text">
                                <small class="text-muted">Last updated 3 mins ago</small>
                            </p>
                        </div>
                    </div>
                    <div class="card">
                        <img
                            class="card-img-top"
                            src=""
                            alt="Card image cap">
                            <div class="card-body">
                                <h5 class="card-title">Card title</h5>
                                <p class="card-text">This is a wider card with supporting text below as a
                                    natural lead-in to additional content. This card has even longer content than
                                    the first to show that equal height action.</p>
                                <button class="sub-btn">Add to Cart</button>
                                <p class="card-text">
                                    <small class="text-muted">Last updated 3 mins ago</small>
                                </p>
                            </div>
                        </div>
                    </div>

                    <%- include('footer'); -%>